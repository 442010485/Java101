import java.util.Scanner;
public class Main{
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        
        int myNum = scan.nextInt();
        myNum = myNum %2;
        
        if (myNum == 0){
        System.out.println("even");           
        }
        else{
            System.out.println("Odd");
        }}}
